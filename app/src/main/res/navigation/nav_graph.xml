<?xml version="1.0" encoding="utf-8"?>
<navigation
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/nav_graph"
        app:startDestination="@id/meFragment">
    <fragment
            android:id="@+id/meFragment"
            android:name="app.getfeeling.feeling.ui.me.MeFragment"
            android:label="MeFragment"
            tools:layout="@layout/me_fragment">
        <action
                android:id="@+id/action_meFragment_to_settings_fragment"
                app:destination="@id/settings_fragment"/>
    </fragment>
    <fragment
            android:id="@+id/settings_fragment"
            android:name="app.getfeeling.feeling.ui.settings.SettingsFragment"
            android:label="Settings Fragment">
        <action
                android:id="@+id/action_settings_fragment_to_meFragment"
                app:destination="@id/meFragment"/>
    </fragment>
</navigation>
